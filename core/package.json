{
  "name": "svelte-tree-view",
  "version": "0.0.6",
  "author": "Teemu Koivisto <teemukoivisto.xyz@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/TeemuKoivisto/svelte-tree-view",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "svelte": "src/index.ts",
  "types": "src/types.d.ts",
  "files": [
    "src",
    "dist",
    ".npmignore",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TeemuKoivisto/svelte-tree-view.git"
  },
  "bugs": {
    "url": "https://github.com/TeemuKoivisto/svelte-tree-view/issues"
  },
  "keywords": [
    "svelte",
    "tree-view",
    "json-tree",
    "tree",
    "json",
    "base16",
    "theme"
  ],
  "scripts": {
    "build": "rimraf ./dist && rollup -c",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|svelte|md|graphql|mdx)\"",
    "lint": "eslint --cache --ext .js,.ts,.svelte ./src",
    "lint:fix": "eslint --fix --ext .js,.ts,.svelte ./src",
    "preversion": "yarn test",
    "version": "yarn build",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "cp ../README.md . && cp ../LICENSE .",
    "postpublish": "rm README.md LICENSE",
    "svelte-check": "svelte-check",
    "test": "jest src",
    "watch": "rollup -cw"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/svelte": "^3.0.3",
    "@types/jest": "^27.0.1",
    "@types/prosemirror-model": "^1.13.2",
    "@types/prosemirror-state": "^1.2.7",
    "@types/prosemirror-view": "^1.18.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-svelte3": "^3.2.0",
    "jest": "^27.1.0",
    "jest-scss-transform": "^1.0.1",
    "node-sass": "^6.0.1",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "prettier-plugin-svelte": "^2.4.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.3",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-ts": "^1.4.1",
    "sass": "^1.41.0",
    "svelte": "^3.42.4",
    "svelte-check": "^2.2.5",
    "svelte-jester": "^1.8.2",
    "svelte-preprocess": "^4.8.0",
    "ts-jest": "^27.0.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.2"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/src/**/*.spec.ts"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "transform": {
      "^.+\\.scss$": "jest-scss-transform",
      "^.+\\.(js|ts)$": "ts-jest",
      "^.+\\.svelte$": [
        "svelte-jester",
        {
          "preprocess": true
        }
      ]
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "svelte"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,svelte}"
    ]
  },
  "dependencies": {}
}
