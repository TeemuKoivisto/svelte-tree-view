{
  "name": "example-app",
  "version": "0.0.1",
  "type": "module",
  "homepage": "https://teemukoivisto.github.io/svelte-tree-view/",
  "scripts": {
    "build": "rimraf build && DEPLOY_TO_GH=true svelte-kit build || true && rimraf build/static && touch build/.nojekyll",
    "deploy": "gh-pages -d build -t true",
    "dev": "svelte-kit dev",
    "format": "prettier --write *.cjs src/",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "preview": "svelte-kit preview"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^1.0.0-next.18",
    "@sveltejs/kit": "1.0.0-next.168",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "gh-pages": "^3.2.3",
    "prettier": "~2.3.2",
    "prettier-plugin-svelte": "^2.4.0",
    "rimraf": "^3.0.2",
    "svelte": "^3.42.4",
    "svelte-preprocess": "^4.8.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.2",
    "vite": "^2.5.1"
  },
  "dependencies": {
    "autoprefixer": "^10.3.3",
    "tailwindcss": "^2.2.8"
  }
}
