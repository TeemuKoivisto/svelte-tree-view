{
  "name": "example-app",
  "version": "0.0.1",
  "type": "module",
  "homepage": "https://teemukoivisto.github.io/svelte-tree-view/",
  "scripts": {
    "build": "rimraf build && svelte-kit build",
    "build:gh-pages": "rimraf build && DEPLOY_TO_GH=true svelte-kit build && touch build/.nojekyll",
    "deploy": "gh-pages -d build -t true",
    "dev": "svelte-kit dev",
    "dev:no-ssr": "NO_SSR=true svelte-kit dev",
    "format": "prettier --write *.cjs src/",
    "lint": "prettier --check --plugin-search-dir=. ./src && eslint --cache --ext .js,.ts,.svelte ./src",
    "lint:fix": "eslint --fix --cache --ext .js,.ts,.svelte ./src",
    "preview": "svelte-kit preview"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^1.0.0-next.21",
    "@sveltejs/kit": "1.0.0-next.184",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "gh-pages": "^3.2.3",
    "prettier": "~2.3.2",
    "prettier-plugin-svelte": "^2.4.0",
    "rimraf": "^3.0.2",
    "svelte": "^3.42.4",
    "svelte-preprocess": "^4.8.0",
    "svelte-tree-view": "*",
    "tslib": "^2.3.1",
    "typescript": "^4.4.2",
    "vite": "^2.5.1"
  },
  "dependencies": {
    "autoprefixer": "^10.3.3",
    "tailwindcss": "^2.2.8"
  }
}
